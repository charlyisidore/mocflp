# Gnuplot script file for plotting boxes

## PLEASE SET THE INSTANCE ##
instance = "F50-52"

set   autoscale                        # scale axes automatically
unset log                              # remove any log-scaling
unset label                            # remove any previous labels
set xtic auto                          # set xtics automatically
set ytic auto                          # set ytics automatically
set title instance." Paving Final"
set xlabel "Obj. 1"
set ylabel "Obj. 2"

set terminal postscript eps color "Times-Roman" 12
set output './plot/'.instance.'_PavingFinal.eps'

set style arrow 1 nohead lw 1 lc rgb 'black'
set pointsize 0.5


#set xr [4500:4800]
#set yr [4500:4800]

plot \
	'./res/'.instance.'_ReconstructionBoxes.out' u 1:2:(0):($4-$2) notitle with vectors as 1, \
	'./res/'.instance.'_ReconstructionBoxes.out' u 1:4:($3-$1):(0) notitle with vectors as 1, \
	'./res/'.instance.'_ReconstructionBoxes.out' u 3:4:(0):($2-$4) notitle with vectors as 1, \
	'./res/'.instance.'_ReconstructionBoxes.out' u 3:2:($1-$3):(0) notitle with vectors as 1
